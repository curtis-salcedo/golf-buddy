<% let totalYardage; %>
<% let totalPar; %>
<section id="scorecard-partials">

<table>
  <tr>
    <td>
      Course Name
    </td>
    <td>
      <%= course.courseName %>
    </td>
  </tr>
  <tr>
    <td>
      Date
    </td>
    <td>
      <%= time.date.toDateString() %>
    </td>
  </tr>
  <tr>
    <td>
      Tee Time
    </td>
    <td>
      <%= time.time %>
    </td>
  </tr>
</table>

<br>

<table>
  <thead>
    <tr>
      <td>hole</td>
        <% course.courseScorecard.forEach(function(s) { %>
          <td><%= s.hole %></td>
        <% }) %>
        <td>FRONT 9</td>
      </tr>
      <tr>
        <td>yardage</td>
        <% course.courseScorecard.forEach(function(s) { %>
            <% totalYardage + s.yardage %>
            <td><%= s.yardage %></td>
          <% }) %>
        <td><%= totalYardage %></td>
        </tr>
      <tr>
        <td>par</td>
        <% course.courseScorecard.forEach(function(s) { %>
            <% totalPar + s.par %>
            <td><%= s.par %></td>
          <% }) %>
        <td><%= totalPar %></td>
    </thead>
  </table>
</section>